#include <PRIZM.h>
PRIZM prizm;

void setup() {
  prizm.PrizmBegin();
  }
  
 
 void loop() {

  prizm.setMotorInvert(2,1);

  prizm.setMotorPower(1,50);
  prizm.setMotorPower(2,50);

  delay(3000);

  prizm.setMotorPower(1,50);
  prizm.setMotorPower(2,0);

  delay(2600);

  prizm.setMotorPower(1,25);
  prizm.setMotorPower(2,25);

    delay(2600);

  prizm.setMotorPower(1,0);
  prizm.setMotorPower(2,50);

  delay(2600);
  
  prizm.setMotorPower(1,50);
  prizm.setMotorPower(2,50);
 
 }
